// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User{
  Id Int @id @default(autoincrement())
  FirstName String
  LastName String
  Password String
  Email String @unique
  CreatedAt DateTime @default(now())
}
